<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="16dp"
                android:color="#583A23" />
        </shape>
    </item>

    <item
        android:left="16dp"
        android:right="16dp"
        android:top="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#6B4D37" />
        </shape>
    </item>

</layer-list>